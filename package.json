  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^1.0.6",
    "@cloudflare/wrangler": "^1.6.0",
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/jest": "^25.1.1",
    "@types/node": "^13.7.0",
    "@types/webpack": "^4.41.3",
    "cross-env": "^7.0.0",
    "husky": "^4.2.1",
    "jest": "^25.1.0",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.1",
    "ts-jest": "^25.1.0",
    "ts-loader": "^9.4.2",
    "ts-node": "^8.6.2",
    "typescript": "^3.7.5",
